{
  "version": 2,
  "functions": {
    "api/bot.py": {
      "runtime": "python3.12"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/web/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
